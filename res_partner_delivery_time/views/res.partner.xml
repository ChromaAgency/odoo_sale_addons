<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_partner_form_inherit_res_partner_stock_time" model="ir.ui.view">
        <field name="name">res.partner.view.form.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <notebook>
                <field name="commercial_partner_id" invisible="1" />
                <page name="elivery_times" string="Horarios de entrega"  >
                        <field name="delivery_time_ids" >
                            <list editable="bottom">
                                <field name="name" />
                                <field name="day_from" />
                                <field name="day_to" />
                                <field name="hour_from"/>
                                <field name="minute_from"/>
                                <field name="hour_to"/>
                                <field name="minute_to"/>
                            </list>
                        </field>
                        
                </page>
                <page name="recception_times" string="Horarios de recepciÃ³n" >
                    <field name="reception_time_ids" >
                        <list editable="bottom">
                            <field name="day_from" />
                            <field name="day_to" />
                            <field name="hour_from"/>
                            <field name="minute_from"/>
                            <field name="hour_to"/>
                            <field name="minute_to"/>
                        </list>
                    </field>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
