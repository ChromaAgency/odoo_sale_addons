<?xml version="1.0" encoding="utf-8"?>
<odoo>

    
<record id="crm_opportunity_report_view_search_inherit_crm_report_enhancement" model="ir.ui.view">
    <field name="name">crm.lead.view.search.inherit</field>
    <field name="model">crm.lead</field>
    <field name="inherit_id" ref="crm.crm_opportunity_report_view_search"/>
    <field name="arch" type="xml">
        <separator position="after">
            <field name="commercial_partner_id" string="Entidad del cliente"/>
            <filter name="commercial_partner_id_group" string="Entidad del cliente" context="{'group_by': 'commercial_partner_id'}" />
            <separator />
            <field name="customer_order_count" string="# de ordenes de cliente"/>
            <separator />
        </separator>
    </field>
</record>
</odoo>
